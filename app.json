{
  "expo": {
    "scheme": "movieapptask",
    "name": "appGainIo-app",
    "slug": "appGainIo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/splash.png"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "intentFilters": [
          {
            "action": "VIEW",
            "autoVerify": true,
            "data": [
               {
                 "scheme": "https",
                 "host": "movieapptask.com",
                 "pathPrefix": "/"
               },
               {
                 "scheme": "movieapptask",
                 "host": "*",
                 "pathPrefix": "/"
               }
             ],
           "category": [
              "BROWSABLE",
              "DEFAULT"
            ]
         }
      ]
      },
      "package": "Mostafa.AppGianIo.com",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fb274698-158d-4f15-bfd8-1c5721b28991"
      }
    },
    "plugins": ["@react-native-firebase/app"]
  }
}
